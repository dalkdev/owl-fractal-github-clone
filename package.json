{
  "name": "fractal-starter",
  "version": "1.0.0",
  "license": "MIT",
  "dependencies": {
    "@frctl/fractal": "^1.5.0",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "embla-carousel": "^8.0.0-rc12",
    "global-prefix": "*",
    "install-with-types": "^0.1.2",
    "smoothscroll-polyfill": "^0.4.4",
    "swiper": "^7.2.0",
    "yarn": "^1.22.19"
  },
  "engines": {
    "node": ">=14 < 15"
  },
  "scripts": {
    "start": "cross-env HMR_PORT=3042 concurrently -n fractal,vite -c magenta,yellow \"yarn fractal:start\" \"yarn vite:start\"",
    "build": "yarn vite:build && fractal build",
    "build:copy": "yarn build && copy build\\app.css ..\\owl-cms-twig\\public\\css\\ && copy build\\app.js ..\\owl-cms-twig\\public\\js\\ && copy build\\vendor.js ..\\owl-cms-twig\\public\\js\\",
    "l:build:copy": "yarn build && cp build/app.css ../owl-cms-twig/public/css/ && cp build/app.js ../owl-cms-twig/public/js/",
    "l:copy": "cp build/app.css ../owl-cms-twig/public/css/ && cp build/app.js ../owl-cms-twig/public/js/",
    "build:serve": "yarn build && serve build",
    "build:upload": "yarn build && winscp.com /script=ftp_upload.scp",
    "fractal:start": "cross-env NODE_ENV=dev fractal start ./ --sync",
    "vite:start": "cross-env  HMR_PORT=3042 vite",
    "vite:build": "tsc && vite build",
    "clean": "node scripts/clean",
    "format": "yarn prettier --write . && yarn stylelint:fix && yarn eslint:fix",
    "stylelint": "stylelint \"src/css/**/*.scss\" \"src/css/**/*.css\" --config .stylelintrc.json",
    "stylelint:fix": "yarn stylelint --fix",
    "eslint": "eslint \"src/js/**/*.ts\" \"src/js/**/*.js\" --config .eslintrc.json",
    "eslint:fix": "yarn eslint --fix",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@types/node": "^18.16.3",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "autoprefixer": "^10.2.6",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "glightbox": "^3.2.0",
    "husky": "^6.0.0",
    "lint-staged": ">=10",
    "moment": "^2.29.1",
    "parcel": "^2.8.3",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "postcss": "^8.3.1",
    "prettier": "2.3.1",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2",
    "sass": "^1.34.1",
    "serve": "^12.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-scss": "^3.19.0",
    "tailwind-scrollbar": "^3.0.0",
    "tailwindcss": "^3.0.0",
    "typescript": "^4.3.2",
    "vite": "^4.0.0"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.css": "stylelint --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
