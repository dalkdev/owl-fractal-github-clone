<div
    class="
    nw-text-sm
    nw-font-medium
    nw-text-black
    nw-bg-white
    nw-rounded-md
    nw-shadow-md
    nw-border
    nw-border-gray-100
  "
>
    <div
        aria-current="true"
        class="
      nw-block
      nw-py-2
      nw-px-4
      nw-w-full
      nw-text-white
      nw-bg-slate-300
      nw-rounded-t-md
      nw-border-b
      nw-border-gray-100
      nw-cursor-pointer
      nw-text-md
      nw-text-left
      nw-font-bold
    "
    >
        Bielefeld
    </div>
    {{#each this.row}}
        <div class="nw-block md:nw-flex">
            <a
                href="#"
                class="
        nw-text-md
        nw-font-medium
        nw-text-black
        nw-text-left
        nw-block
        nw-py-2
        nw-px-4
        nw-w-full
        nw-border-b
        nw-border-r-0
        md:nw-border-r
        nw-border-gray-100 nw-cursor-pointer
        hover:nw-bg-gray-0
      "
            >
                {{this.cell1}}
                <svg
                    xmlns="http://www.w3.org/2000/svg"
                    class="nw-float-right nw-text-black nw-mt-0.5"
                    width="17"
                    height="17"
                    fill=" none
                "
                    viewBox="0 0 24 24"
                    stroke="currentColor"
                    stroke-width="2"
                >
                    <path stroke-linecap="round" stroke-linejoin="round" d="M9 5l7 7-7 7"/>
                </svg>
            </a>
            <a
                href="#"
                class="
        nw-text-md
        nw-font-medium
        nw-text-black
        nw-text-left
        nw-block
        nw-py-2
        nw-px-4
        nw-w-full
        nw-border-b
        nw-border-gray-100
        nw-cursor-pointer
        hover:nw-bg-gray-0
      "
            >
                {{this.cell2}}
                <svg
                    xmlns="http://www.w3.org/2000/svg"
                    class="nw-float-right nw-text-black nw-mt-0.5"
                    width="17"
                    height="17"
                    fill=" none
                "
                    viewBox="0 0 24 24"
                    stroke="currentColor"
                    stroke-width="2"
                >
                    <path stroke-linecap="round" stroke-linejoin="round" d="M9 5l7 7-7 7"/>
                </svg>
            </a>
        </div>
    {{/each}}
</div>
