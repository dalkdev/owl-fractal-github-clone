                x.classList.remove('nw-text-red-0', 'nw-bg-white', 'hover:nw-text-white', 'hover:nw-bg-red-0');
                x.classList.add('nw-text-white', 'nw-bg-red-0', 'hover:nw-text-red-0', 'hover:nw-bg-white');
            });
            x.classList.remove('nw-text-white', 'nw-bg-red-0', 'hover:nw-text-red-0', 'hover:nw-bg-white');
            x.classList.add('nw-text-red-0', 'nw-bg-white', 'hover:nw-text-white', 'hover:nw-bg-red-0');
            let array_container = document.getElementsByClassName('container');
            Array.prototype.forEach.call(array_container, x => {
                x.classList.add('nw-hidden');
            });
            array_container[i].classList.remove('nw-hidden');
        });
    });
</script>
