<div class="nw-mx-4 sm:nw-mx-0 md:nw-mx-0">
  <div
    class="
      nw-pt-12
      nw-pb-6
      nw-text-center
      nw-font-roboto
      nw-font-bold
      nw-text-black
      nw-text-2xl
      nw-tracking-normal
      nw-leading-6
      nw-uppercase
    "
  >
    Bielefeld
  </div>
  <!-- grid -->
  <div
    class="
      nw-grid nw-grid-cols-1
      md:nw-grid-cols-3
      nw-gap-4 nw-pt-6 nw-flex nw-flex-col nw-flex-row nw-justify-between
    "
  >
    <!-- box1 -->
    {{>'@single-thema-box'}}
    <!-- box1 Ende -->
  </div>
  <!-- buttons -->
  {{>'@themenglossar-buttons'}}
  <!-- buttons Ende -->
  <div class="nw-mt-8 nw-grid nw-grid-cols-1 md:nw-grid-cols-3 nw-gap-4">
    <div class="nw-col-span-2">
      <div class="nw-relative nw-overflow-x-auto nw-shadow-md nw-rounded-md">
        <table class="nw-w-full nw-text-sm nw-text-left nw-text-gray-black">
          <thead class="nw-text-base nw-text-white nw-bg-custom-1-700">
            <tr>
              <th colspan="2" scope="col" class="nw-px-4 nw-py-2">Bielefeld</th>
            </tr>
          </thead>
          <tbody>
            {{#each this.row}}
            <tr class="nw-bg-white nw-text-base nw-font-medium nw-text-left">
              <td class="nw-px-4 nw-py-2 nw-border nw-border-gray-200">
                <a href="#">
                  {{this.cell1}}
                  <svg
                    xmlns="http://www.w3.org/2000/svg"
                    class="nw-float-right nw-text-black nw-mt-0.5"
                    width="17"
                    height="17"
                    fill=" none
                "
                    viewBox="0 0 24 24"
                    stroke="currentColor"
                    stroke-width="2"
                  >
                    <path
                      stroke-linecap="round"
                      stroke-linejoin="round"
                      d="M9 5l7 7-7 7"
                    />
                  </svg>
                </a>
              </td>
              <td class="nw-px-4 nw-py-2 nw-border nw-border-gray-200">
                <a href="#">
                  {{this.cell2}}
                  <svg
                    xmlns="http://www.w3.org/2000/svg"
                    class="nw-float-right nw-text-black nw-mt-0.5"
                    width="17"
                    height="17"
                    fill=" none
                "
                    viewBox="0 0 24 24"
                    stroke="currentColor"
                    stroke-width="2"
                  >
                    <path
                      stroke-linecap="round"
                      stroke-linejoin="round"
                      d="M9 5l7 7-7 7"
                    />
                  </svg>
                </a>
              </td>
            </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
    </div>

    <div>
      <div class="">
        <div class="nw-bg-white nw-mx-auto nw-w-72 nw-h-64 nw-rounded-md"></div>
      </div>
      <div class="nw-mt-4">
        {{#each this.lokal}}
        <div
          class="
            nw-text-sm
            nw-font-medium
            nw-text-gray-900
            nw-bg-white
            nw-rounded-md
            nw-shadow-md
            nw-border
            nw-border-gray-200
          "
        >
          <div
            aria-current="true"
            class="
              nw-block
              nw-py-2
              nw-px-4
              nw-w-full
              nw-text-white
              nw-bg-custom-1-700
              nw-rounded-t-md
              nw-border-b
              nw-border-gray-200
              nw-cursor-pointer
              nw-text-base
              nw-text-left
              nw-font-bold
            "
          >
            {{this.label}}
          </div>
          {{#each this.items}}
          <a
            href="#"
            class="
              nw-text-base
              nw-font-medium
              nw-text-black
              nw-text-left
              nw-block
              nw-py-2
              nw-px-4
              nw-w-full
              nw-border-b
              nw-border-gray-200
              nw-cursor-pointer
              hover:nw-bg-gray-100
              focus:nw-outline-none
              focus:nw-ring-2
              focus:nw-ring-gray-200
              focus:nw-text-custom-1-700
              hover:nw-bg-gray-200 hover:nw-nw-text-white
              focus:nw-ring-gray-500 focus:nw-text-white
            "
          >
            {{this}}
            <svg
              xmlns="http://www.w3.org/2000/svg"
              class="nw-float-right nw-text-black nw-mt-0.5"
              width="17"
              height="17"
              fill=" none
                "
              viewBox="0 0 24 24"
              stroke="currentColor"
              stroke-width="2"
            >
              <path
                stroke-linecap="round"
                stroke-linejoin="round"
                d="M9 5l7 7-7 7"
              />
            </svg>
          </a>
          {{/each}}
        </div>
        {{/each}}
      </div>
    </div>
  </div>
  <div class="nw-grid nw-grid-cols-1 md:nw-grid-cols-3 nw-gap-4">
    <div class="nw-col-span-2">
      {{#each this.stadt}}
      <div
        class="
          nw-mt-4 nw-relative nw-overflow-x-auto nw-shadow-md nw-rounded-md
        "
      >
        <table class="nw-w-full nw-text-sm nw-text-left nw-text-gray-black">
          <thead class="nw-text-base nw-text-white nw-bg-custom-1-700">
            <tr>
              <th colspan="2" scope="col" class="nw-px-4 nw-py-2">
                {{this.label}}
              </th>
            </tr>
          </thead>
          <tbody>
            {{#each this.items}}
            <tr class="nw-bg-white nw-text-base nw-font-medium nw-text-left">
              <td class="nw-px-4 nw-py-2 nw-border nw-border-gray-200">
                <a href="#">
                  {{this.cell1}}
                  <svg
                    xmlns="http://www.w3.org/2000/svg"
                    class="nw-float-right nw-text-black nw-mt-0.5"
                    width="17"
                    height="17"
                    fill=" none
                "
                    viewBox="0 0 24 24"
                    stroke="currentColor"
                    stroke-width="2"
                  >
                    <path
                      stroke-linecap="round"
                      stroke-linejoin="round"
                      d="M9 5l7 7-7 7"
                    />
                  </svg>
                </a>
              </td>
              <td class="nw-px-4 nw-py-2 nw-border nw-border-gray-200">
                <a href="#">
                  {{this.cell2}}
                  <svg
                    xmlns="http://www.w3.org/2000/svg"
                    class="nw-float-right nw-text-black nw-mt-0.5"
                    width="17"
                    height="17"
                    fill=" none
                "
                    viewBox="0 0 24 24"
                    stroke="currentColor"
                    stroke-width="2"
                  >
                    <path
                      stroke-linecap="round"
                      stroke-linejoin="round"
                      d="M9 5l7 7-7 7"
                    />
                  </svg>
                </a>
              </td>
            </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
      {{/each}}
    </div>
  </div>
</div>
