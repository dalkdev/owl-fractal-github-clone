<div class="">
  <div class="nw-py-2 nw-flex nw-mx-2">
    <a href="">
      <svg
        xmlns="http://www.w3.org/2000/svg"
        width="20"
        height="20"
        class="nw-text-gray-500"
        viewBox="0 0 20 20"
        fill="currentColor"
      >
        <path
          d="M10.707 2.293a1 1 0 00-1.414 0l-7 7a1 1 0 001.414 1.414L4 10.414V17a1 1 0 001 1h2a1 1 0 001-1v-2a1 1 0 011-1h2a1 1 0 011 1v2a1 1 0 001 1h2a1 1 0 001-1v-6.586l.293.293a1 1 0 001.414-1.414l-7-7z"
        />
      </svg>
    </a>

    <svg
      xmlns="http://www.w3.org/2000/svg"
      width="20"
      height="20"
      class="nw-text-gray-500 nw-ml-2"
      viewBox="0 0 20 20"
      fill="currentColor"
    >
      <path
        fill-rule="evenodd"
        d="M7.293 14.707a1 1 0 010-1.414L10.586 10 7.293 6.707a1 1 0 011.414-1.414l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414 0z"
        clip-rule="evenodd"
      />
    </svg>
    <a href="" class="nw-text-black nw-ml-2 nw-text-sm"> Sport </a>
    <svg
      xmlns="http://www.w3.org/2000/svg"
      width="20"
      height="20"
      class="nw-text-gray-500 nw-ml-2"
      viewBox="0 0 20 20"
      fill="currentColor"
    >
      <path
        fill-rule="evenodd"
        d="M7.293 14.707a1 1 0 010-1.414L10.586 10 7.293 6.707a1 1 0 011.414-1.414l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414 0z"
        clip-rule="evenodd"
      />
    </svg>
    <a href=" " class="nw-text-black nw-ml-2 nw-text-sm">
      DSC Arminia Bielefeld
    </a>
  </div>
  <!-- Arminia Bielefeld -->
  <div
    class="nw-bg-custom-600 nw-mx-2 nw-my-2 nw-pt-4 nw-rounded-md nw-shadow-md"
  >
    <div class="nw-flex-shrink-0">
      <img
        class="nw-w-24 nw-h-24 nw-rounded-full nw-mx-auto"
        src="{{this.image-src-s}}"
        alt=""
      />
    </div>
    <div>
      <p
        class="
          nw-text-center nw-text-xl nw-font-bold nw-pt-4 nw-pb-3 nw-text-white
        "
      >
        {{title}}
      </p>
    </div>
    <!--buttons -->
    <div class="nw-pr-auto nw-max-w-lg nw-px-2">
      {{#each buttons}}
      <a
        href=""
        class="
          button
          nw-text-white
          nw-inline-block
          nw-border
          nw-border-gray-200
          nw-border-solid
          nw-rounded-3xl
          nw-pt-1.5
          nw-pb-1.5
          nw-px-3
          nw-font-bold
          nw-text-sm
          nw-text-left
          nw-mt-1
        "
      >
        {{this}}
      </a>
      {{/each}}
    </div>
    <!-- Buttons Ende -->
    <div class="nw-bg-white nw-pb-4 nw-rounded-b-md nw-shadow-md">
      <div class="nw-flex nw-mt-2 nw-py-4 nw-place-content-center">
        <div class="nw-flex-shrink-0 nw-mr-2">
          <img
            class="nw-w-16 nw-h-16 nw-rounded-full"
            src="{{this.image-src-s}}"
            alt=""
          />
        </div>
        <div
          class="
            nw-py-1.5 nw-text-black nw-font-bold nw-text-base nw-text-center
          "
        >
          17.8.2022
          <div class="nw-font-normal nw-text-tiny">20:30 Uhr</div>
        </div>
        <div class="nw-flex-shrink-0 nw-ml-2">
          <img
            class="nw-w-16 nw-h-16 nw-rounded-full"
            src="{{this.image-src-s}}"
            alt=""
          />
        </div>
      </div>
      <div class="nw-text-center nw-pb-2 nw-px-16">
        <span class="nw-font-bold">DSC Arminia Bielefeld </span>
        <span> spielt gegen </span>
        <span class="nw-font-bold"> FC Bayern München </span> in:
      </div>
      <div class="nw-flex nw-justify-around nw-px-1">
        {{#each time-button}}
        <a
          class="
            nw-py-2
            nw-w-20
            nw-m-0.5
            nw-bg-gray-300
            nw-text-center
            nw-text-white
            nw-inline-block
            nw-rounded-md
          "
        >
          {{this.part1}}<br />
          {{this.part2}}</a
        >
        {{/each}}
      </div>
      <!--live-button -->
      {{>'@live-button'}}
      <!--live-button Ende -->
    </div>
  </div>
  <!-- Arminia Bielefeld Ende -->
  <!-- Arminia-Bielefeld-News-->
  {{>'@arminia-bielefeld-news'}}
  <!--Arminia-Bielefeld-News Ende -->
  <!-- Weitere Artkel anzeigen-->
