    <a href=" " class="nw-text-black nw-ml-2 nw-text-sm">
      DSC Arminia Bielefeld
    </a>
  </div>
  <!-- Arminia Bielefeld -->
  <div
    class="nw-bg-custom-600 nw-mx-2 nw-my-2 nw-pt-4 nw-rounded-md nw-shadow-md"
  >
    <div class="nw-flex-shrink-0">
      <img
        class="nw-w-24 nw-h-24 nw-rounded-full nw-mx-auto"
        src="{{this.image-src-s}}"
        alt=""
      />
    </div>
    <div>
      <p
        class="
          nw-text-center nw-text-xl nw-font-bold nw-pt-4 nw-pb-3 nw-text-white
        "
      >
        {{title}}
      </p>
    </div>
    <!--buttons -->
    <div class="nw-pr-auto nw-max-w-lg nw-px-2">
      {{#each buttons}}
      <a
        href=""
        class="
          button
          nw-text-white
          nw-inline-block
          nw-border
          nw-border-gray-200
          nw-border-solid
          nw-rounded-3xl
          nw-pt-1.5
          nw-pb-1.5
          nw-px-3
          nw-font-bold
          nw-text-sm
          nw-text-left
          nw-mt-1
        "
      >
        {{this}}
      </a>
      {{/each}}
    </div>
    <!-- Buttons Ende -->
    <div class="nw-bg-white nw-pb-4 nw-rounded-b-md nw-shadow-md">
      <div class="nw-flex nw-mt-2 nw-py-4 nw-place-content-center">
        <div class="nw-flex-shrink-0 nw-mr-2">
          <img
            class="nw-w-16 nw-h-16 nw-rounded-full"
            src="{{this.image-src-s}}"
            alt=""
          />
        </div>
        <div
          class="
            nw-py-1.5 nw-text-black nw-font-bold nw-text-base nw-text-center
          "
        >
          17.8.2022
          <div class="nw-font-normal nw-text-tiny">20:30 Uhr</div>
        </div>
        <div class="nw-flex-shrink-0 nw-ml-2">
          <img
            class="nw-w-16 nw-h-16 nw-rounded-full"
            src="{{this.image-src-s}}"
            alt=""
          />
        </div>
      </div>
      <div class="nw-text-center nw-pb-2 nw-px-16">
        <span class="nw-font-bold">DSC Arminia Bielefeld </span>
        <span> spielt gegen </span>
        <span class="nw-font-bold"> FC Bayern MÃ¼nchen </span> in:
      </div>
      <div class="nw-flex nw-justify-around nw-px-1">
        {{#each time-button}}
        <a
          class="
            nw-py-2
            nw-w-20
            nw-m-0.5
            nw-bg-gray-300
            nw-text-center
            nw-text-white
            nw-inline-block
            nw-rounded-md
          "
        >
          {{this.part1}}<br />
          {{this.part2}}</a
        >
        {{/each}}
      </div>
      <div
        class="
          nw-text-center nw-pb-3 nw-pt-3 nw-px-16 nw-text-black nw-font-bold
        "
      >
        <div class="nw-text-tiny">Hier geht es zum</div>
        <div class="nw-flex nw-place-content-center">
          <div class="nw-w-3 nw-h-3 nw-rounded-full nw-bg-red-500"></div>
          <span class="nw-ml-1 nw-text-tiny nw-uppercase">Live-Tricker</span>
        </div>
      </div>
      <a
        href=""
        class="
          nw-mx-auto
          nw-w-1/2
          nw-bg-red-500
          nw-border
          nw-border-transparent
          nw-rounded-md
          nw-py-1
          nw-px-4
          nw-flex
          nw-items-center
          nw-justify-center
          nw-text-tiny
          nw-font-medium
          nw-text-white
          hover:nw-bg-red-600
          focus:nw-outline-none
          focus:nw-ring-2
          focus:nw-ring-offset-2
          focus:nw-ring-offset-gray-800
          focus:nw-ring-indigo-500
        "
      >
        {{label}}
      </a>
    </div>
  </div>
