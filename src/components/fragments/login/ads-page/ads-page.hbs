<div class="nw-bg-white nw-pb-4 nw-mx-0 nw-px-4">
  <div>
    <p
      class="
        nw-text-left
        nw-text-base
        nw-font-roboto
        nw-font-bold
        nw-opacity-100
        nw-text-black
        nw-pt-4
        nw-pb-1
        nw-tracking-normal
      "
    >
      {{headline}}
    </p>
  </div>

  <div id="">
    <div class=""></div>
    <div class="nw-max-w-full nw-mb-6 nw-bg-white">
      <div
        class="
          nw-flex
          nw-justify-center
          nw-content-center
          nw-mb-1
          nw-ml-auto
          nw-mr-auto
        "
      >
        <div
          class="
            nw-mt-6
            nw-bg-gray-600
            nw-relative
            nw-text-white
            nw-text-center
            nw-w-10
            nw-h-10
            nw-rounded-full
            nw-flex
            nw-content-center
            nw-justify-center
          "
        >
          <span
            style="transform: translateY(-50%)"
            class="nw-absolute nw-top-2/4"
            >FW</span
          >
        </div>
      </div>
      <div
        class="
          nw-px-4
          nw-font-bold
          nw-font-roboto
          nw-text-black
          nw-text-base
          nw-tracking-normal
          nw-text-center
        "
      >
        Willkommen,
        <br />
        {{name}}
      </div>
    </div>
  </div>
  <!-- Für Mobile-->
  <!-- button1-->
  <div class="nw-block md:nw-hidden">
    <div class="nw-mb-4">
      <select class="nw-select-icon-plus">
        <option value="">Anzeigen erstellen</option>
        <option value="">Anzeigen verwalten</option>
      </select>
    </div>

    <!--button2-->
    <a
      class="
        nw-inline-flex
        nw-w-full
        nw-px-2
        nw-py-1
        nw-text-tiny
        nw-font-medium
        nw-tracking-normal
        nw-inline-block
        nw-border-2
        nw-border-solid
        nw-rounded
        nw-text-tiny
        nw-box-border
        nw-cursor-pointer
      "
      type="text"
      id=""
    >
      <svg
        class="nw-mr-0.5 nw-cursor-pointer"
        width="25"
        height="25"
        xmlns="http://www.w3.org/2000/svg"
        fill="none"
        viewBox="0 0 24 24"
        stroke="currentColor"
      >
        <path
          stroke-linecap="round"
          stroke-linejoin="round"
          stroke-width="2"
          d="M12 9v3m0 0v3m0-3h3m-3 0H9m12 0a9 9 0 11-18 0 9 9 0 0118 0z"
        />
      </svg>
      Anzeigen erstellen
    </a>
    <!--bitton3 -->
    <div class="nw-relative nw-my-4">
      <button
        class="
          nw-inline-flex nw-items-center nw-w-full nw-h-10
          px-5
          nw-text-black
          nw-transition-colors
          nw-border-2
          nw-border-gray-200
          nw-duration-150
          nw-bg-white
          nw-rounded
          hover:nw-bg-gray-400
        "
      >
        <svg
          class="nw-ml-2 nw-mr-0.5 nw-cursor-pointer"
          width="25"
          height="25"
          xmlns="http://www.w3.org/2000/svg"
          fill="none"
          viewBox="0 0 24 24"
          stroke="currentColor"
        >
          <path
            stroke-linecap="round"
            stroke-linejoin="round"
            stroke-width="2"
            d="M12 9v3m0 0v3m0-3h3m-3 0H9m12 0a9 9 0 11-18 0 9 9 0 0118 0z"
          />
        </svg>
        <span>Diese ist keine Wahl nur Title</span>
        <svg
          class="nw-w-4 nw-h-4 nw-ml-auto nw-fill-current"
          viewBox="0 0 20 20"
        >
          <path
            d="M5.293 7.293a1 1 0 011.414 0L10 10.586l3.293-3.293a1 1 0 111.414 1.414l-4 4a1 1 0 01-1.414 0l-4-4a1 1 0 010-1.414z"
            clip-rule="evenodd"
            fill-rule="evenodd"
          ></path>
        </svg>
      </button>
    </div>
  </div>

  <!--
          <button
              class="nw-bg-gray-300 hover:nw-bg-gray-400 nw-text-gray-800 nw-font-bold nw-py-2 nw-px-4 nw-rounded nw-inline-flex
              nw-items-center">
              <svg
                  class="nw-mx-auto nw-text-custom-1-700 nw-cursor-pointer"
                  width="30"
                  height="30"
                  xmlns="http://www.w3.org/2000/svg"
                  fill="none"
                  viewBox="0 0 24 24"
                  stroke="currentColor"
              >
                  <path
                      stroke-linecap="round"
                      stroke-linejoin="round"
                      stroke-width="2"
                      d="M12 9v3m0 0v3m0-3h3m-3 0H9m12 0a9 9 0 11-18 0 9 9 0 0118 0z"
                  />
              </svg>
              <span>Download</span>
          </button>
          -->

  <!-- Für Desktop-->
  <div class="nw-hidden md:nw-block">
    <ul
      class="
        nw-nav nw-nav-tabs nw-flex nw-flex-col nw-text-gray-400
        md:nw-flex-row
        nw-flex-wrap
        nw-list-none
        nw-border-b
        nw-border-gray-200
        nw-pl-0
        nw-pb-
        nw-mb-4
      "
      id=""
      role="tablist"
    >
      <li class="nw-nav-item nw-flex-auto nw-text-right" role="tab">
        <a
          id="aErstellen"
          class="
            nw-font-roboto
            nw-font-medium
            nw-uppercase
            nw-tracking-normal
            nw-py-2
            nw-px-4
            nw-inline-flex
            nw-text-sm
            nw-cursor-pointer
            hover:nw-border-solid hover:nw-border-black hover:nw-border-b-2
            nw-items-center
            hover:nw-text-black
            focus:nw-text-black
          "
        >
          <svg
            id="svgErstellen"
            class="nw-mr-1.5 nw-cursor-pointer"
            width="30"
            height="30"
            xmlns="http://www.w3.org/2000/svg"
            fill="none"
            viewBox="0 0 24 24"
            stroke="currentColor"
          >
            <path
              stroke-linecap="round"
              stroke-linejoin="round"
              stroke-width="2"
              d="M12 9v3m0 0v3m0-3h3m-3 0H9m12 0a9 9 0 11-18 0 9 9 0 0118 0z"
            />
          </svg>
          <span> Anzeigen erstellen</span></a
        >
      </li>
      <li class="nw-nav-item nw-flex-auto nw-text-left" role="tab">
        <a
          id="aVerwalten"
          class="
            nw-font-roboto
            nw-font-medium
            nw-uppercase
            nw-tracking-normal
            nw-py-2
            nw-px-4
            nw-inline-flex
            nw-text-sm
            nw-cursor-pointer
            nw-border-b-2
            nw-border-solid
            nw-border-white
            hover:nw-border-solid hover:nw-border-gray-600 hover:nw-border-b-2
            nw-items-center
            hover:nw-text-black
            focus:nw-text-black
          "
        >
          <svg
            id="svgVerwalten"
            class="nw-mr-1.5 nw-cursor-pointer"
            width="30"
            height="30"
            xmlns="http://www.w3.org/2000/svg"
            fill="none"
            viewBox="0 0 24 24"
            stroke="currentColor"
          >
            <path
              stroke-linecap="round"
              stroke-linejoin="round"
              stroke-width="2"
              d="M11 5H6a2 2 0 00-2 2v11a2 2 0 002 2h11a2 2 0 002-2v-5m-1.414-9.414a2 2 0 112.828 2.828L11.828 15H9v-2.828l8.586-8.586z"
            />
          </svg>
          <span>Anzeigen verwalten </span>
        </a>
      </li>
    </ul>
  </div>
  <!--div fur alle-->
</div>

<script>
  var iconerstellen = document.getElementById('aErstellen');
  var iconverwalten = document.getElementById('aVerwalten');
  var svg1 = document.getElementById('svgErstellen');
  var svg2 = document.getElementById('svgVerwalten');

  iconerstellen.addEventListener('mouseover', mouseOver1);
  iconerstellen.addEventListener('mouseout', mouseOut1);
  iconverwalten.addEventListener('mouseover', mouseOver2);
  iconverwalten.addEventListener('mouseout', mouseOut2);

  function mouseOver1() {
    iconerstellen.classList.add('nw-text-black-500');
    svg1.classList.add('nw-text-custom-1-700');
  }

  function mouseOut1() {
    iconerstellen.classList.remove('nw-text-black-500');
    svg1.classList.remove('nw-text-custom-1-700');
  }

  function mouseOver2() {
    iconverwalten.classList.add('nw-text-black-500');
    svg2.classList.add('nw-text-custom-1-700');
  }

  function mouseOut2() {
    iconverwalten.classList.remove('nw-text-black-500');
    svg2.classList.remove('nw-text-custom-1-700');
  }
</script>
