<li class="nw-nav-item">
  <div>
    <a href="#">{{label}}</a>
    <div class="nw-menu-seperator"></div>
    {{>'@dropdown-indicator'}}
  </div>
  {{#each menus}}
  <ul>
    <li>
      <div class="nw-nav-item-2nd">
        <a
          href="#"
          class="nw-text-black nw-text-sm nw-pl-6 nw-font-bold nw-flex-grow"
        >
          {{#if this.menus}} {{this.label}} {{else}} {{this}} {{/if}}
        </a>
        {{#if this.menus}}
        <div class="nw-menu-seperator"></div>
        {{>'@dropdown-indicator'}}
      </div>
      <ul class="nw-nav-item-3rd">
        {{#each this.menus}}
        <li class="nw-py-1.5">{{this}}</li>
        {{/each}}
      </ul>
      {{/if}}
    </li>
  </ul>
  {{/each}}
</li>
